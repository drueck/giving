<div class="page-header">
  <h1>Users</h1>
</div>
<div class="row">
  <div class="span6">
    <table class="table table-striped table-bordered">
      <thead>
        <tr>
          <th>Username</th>
          <th>Type</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= link_to user.username, edit_user_path(user) %></td>
            <td><%= user.user_type %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
<%= will_paginate @users %>
<%= link_to 'New User', new_user_path, :class => 'btn' %>
