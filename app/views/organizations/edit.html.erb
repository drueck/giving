<h1>Organization Information</h1>
<%= simple_form_for @organization, url: organization_path do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :city %>
  <%= f.input :state %>
  <%= f.input :zip %>
  <%= f.input :phone %>
  <%= f.submit 'Save', class: 'btn btn-default' %>
  <%= link_to 'Cancel', root_url, class: 'btn btn-default' %>
<% end %>
